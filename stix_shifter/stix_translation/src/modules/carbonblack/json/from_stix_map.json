      "end": ["endtime"]
    }
  },
  "username": {
    "fields": {
      "user_id": ["username"]
    }
  },
  "artifact": {
    "fields": {
      "payload_bin": ["payload"]
    }
  },
  "domain": {
    "fields": {
      "value": ["domainname"]
    }
  }
}
